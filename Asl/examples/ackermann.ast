(LIST_FUNCTIONS (func ack (PARAMS m n) (LIST_INSTR (if (= m 0) (LIST_INSTR (return (+ n 1)))) (if (= n 0) (LIST_INSTR (return (FUNCALL ack (ARGLIST (- m 1) 1))))) (return (FUNCALL ack (ARGLIST (- m 1) (FUNCALL ack (ARGLIST m (- n 1)))))))) (func main PARAMS (LIST_INSTR (write "Enter m and n: ") (read m) (read n) (write "A(") (write m) (write ",") (write n) (write ")=") (write (FUNCALL ack (ARGLIST m n))) (write "%n"))))